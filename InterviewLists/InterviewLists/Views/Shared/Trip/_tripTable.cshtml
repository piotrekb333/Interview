@model IEnumerable<InterviewLists.Application.Models.Trip.TripDto>


<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Price</th>
            <th>Date Start</th>
            <th>Date End</th>
            <th>Country</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Price</td>
                <td>@item.DateStart</td>
                <td>@item.DateEnd</td>
                <td>@item.Country</td>
                <td>
                    <a href="#" data-id="@item.Id" class="edit edit-trip"><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
                    <a href="#" data-id="@item.Id" class="delete delete-trip"><i class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
                </td>
            </tr>
        }

    </tbody>
</table>